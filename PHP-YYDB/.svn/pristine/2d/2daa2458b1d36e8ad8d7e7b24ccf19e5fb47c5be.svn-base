    <script type="text/javascript" src="js/yydb.js"></script>
</head>

<body>
<!--一元夺宝首页-->
    <div class="list_head">
        <span>返回</span>
        <span>订单中心页面</span>
        <div class="icon_right rf">
            <a href="#"><i></i></a>
            <a href="#"><i class="sx"></i></a>
        </div>
    </div>

<div class="container">
    <!-- 轮播图开始-->
    <div class="slide">
        <div class="swiper-wrapper">
            <div class="swiper-slide">
                <a href="#">
                    <img src="images/h01.jpg" alt=""/>
                </a>
            </div>
            <div class="swiper-slide">
                <a href="#">
                    <img src="images/h02.jpg" alt=""/>
                </a>
            </div>
        </div>
        <div class="pagination"></div>
    </div>
    <!-- 轮播图结束-->

  

    <div class="sy-fanhui">
        <a href="JavaScript:0;"><img src="images/sjsc29.gif"></a>
    </div>
</div>

